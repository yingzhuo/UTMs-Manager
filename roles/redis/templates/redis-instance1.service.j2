[Unit]
Description=Redis ({{ instance1.port }})
Documentation=https://redis.io/docs/
After=syslog.target network.target

[Service]
Type=simple
User=root
Group=root
ExecStart=/opt/redis/bin/redis-server /opt/redis/etc/redis-{{ instance1.port }}.conf
KillSignal=15
SuccessExitStatus=143

[Install]
WantedBy=multi-user.target
